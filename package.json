{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "watch": {
    "test": "src/**/*.ts"
  },
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "test:browser": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "test": "npm run build && concurrently \"npm run build:watch\" \"mocha --watch --watch-extensions js src/**/*.spec.js\"",
    "pretest:only": "npm run build",
    "test:only": "mocha src/**/*.spec.js",
    "test:old": "mocha --require ts-node/register --watch --watch-extensions js,ts \"src/**/*.spec.js\"",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "watch": "npm-watch",
    "clean:tsout": "rimraf src/*.js.map src/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.3.4",
    "@angular/compiler": "~4.3.4",
    "@angular/core": "~4.3.4",
    "@angular/forms": "~4.3.4",
    "@angular/http": "~4.3.4",
    "@angular/platform-browser": "~4.3.4",
    "@angular/platform-browser-dynamic": "~4.3.4",
    "@angular/router": "~4.3.4",
    "angular-in-memory-web-api": "~0.3.0",
    "core-js": "^2.4.1",
    "mongoose": "^4.12.1",
    "portfinder": "^1.0.13",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/mongodb": "^2.2.13",
    "@types/mongoose": "^4.7.23",
    "@types/node": "^6.0.46",
    "@types/systemjs": "^0.20.6",
    "canonical-path": "0.0.2",
    "chai": "^4.1.2",
    "concurrently": "^3.2.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "mocha": "^4.0.1",
    "npm-watch": "^0.3.0",
    "rimraf": "^2.5.4",
    "should": "^13.1.2",
    "sinon": "^4.0.1",
    "testdouble": "^3.2.6",
    "ts-node": "^3.3.0",
    "tslint": "^3.15.1",
    "typescript": "~2.5.3"
  },
  "repository": {}
}
